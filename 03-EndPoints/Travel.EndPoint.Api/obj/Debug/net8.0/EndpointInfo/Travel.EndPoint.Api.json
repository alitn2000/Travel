{
  "openapi": "3.0.1",
  "info": {
    "title": "Travel.EndPoint.Api",
    "version": "1.0"
  },
  "paths": {
    "/api/CheckList/GetAllCheckLists": {
      "get": {
        "tags": [
          "CheckList"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Trip/AddTrip": {
      "post": {
        "tags": [
          "Trip"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AddTripModel"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/AddTripModel"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/AddTripModel"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Trip/UsersTrips": {
      "get": {
        "tags": [
          "Trip"
        ],
        "parameters": [
          {
            "name": "userId",
            "in": "query",
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "AddTripModel": {
        "required": [
          "destination",
          "end",
          "start",
          "tripType",
          "userId"
        ],
        "type": "object",
        "properties": {
          "destination": {
            "minLength": 1,
            "type": "string"
          },
          "start": {
            "type": "string",
            "format": "date-time"
          },
          "end": {
            "type": "string",
            "format": "date-time"
          },
          "tripType": {
            "$ref": "#/components/schemas/TripEnums"
          },
          "userId": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "TripEnums": {
        "enum": [
          1,
          2,
          3,
          4,
          5
        ],
        "type": "integer",
        "format": "int32"
      }
    }
  }
}